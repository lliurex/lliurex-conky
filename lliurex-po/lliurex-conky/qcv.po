msgid "CONKY CONFIGUTATOR. SELECT THE FORMAT THAT YOU LIKE"
msgstr "CONFIGURADOR DEL CONKY, SELECCIONEU EL FORMAT QUE MÉS US AGRADE"

#: conkyrc_gui:27
msgid "<b><big>INFO </big></b>CONKY TEXT: <small>Select your favourite colour or personalice it for INFO</small>"
msgstr ""
"<b><big>INFO </big></b>TEXT DEL CONKY: <small>per a INFO, seleccioneu el "
"color preferit o personalitzeu-lo</small>"

